{
  "name": "@pleme-io/config-manager",
  "version": "1.0.1",
  "description": "Runtime configuration management with window.ENV pattern",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "check": "biome check --diagnostic-level=warn src",
    "check:types": "tsc --noEmit",
    "format": "biome check --write src",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.0",
    "@vitest/ui": "^3.2.0",
    "@biomejs/biome": "^1.9.4",
    "jsdom": "^27.0.0",
    "tsdown": "^0.2.0",
    "typescript": "^5.7.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.0"
  },
  "keywords": [
    "config",
    "configuration",
    "runtime",
    "environment",
    "typescript",
    "library",
    "nexus",
    "pleme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pleme-io/pleme-config-manager.git"
  },
  "license": "MIT",
  "author": "Pleme.io Team",
  "module": "./dist/index.js"
}
